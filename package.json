{
  "name": "auto-apply",
  "displayName": "Auto apply",
  "version": "0.0.1",
  "description": "To automate dice application for now, send that URL to API for a cold email",
  "author": "Blazing Bane",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://dice.com/*"
    ],
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "matches": [
      "https://dice.com/*"
    ]
  }
